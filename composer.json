{
    "name": "alichry/laminas-authorization",
    "description": "Authorization for Laminas",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Cherry",
            "email": "cmcrc@alicherry.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-servicemanager": "^3.4",
        "alichry/laminas-accesscontrol": "^1.0.0",
        "laminas/laminas-session": "^2.9",
        "doctrine/annotations": "^1.10",
        "laminas/laminas-mvc": "3.1.1"
    },
    "autoload": {
        "psr-4": {
            "AliChry\\Laminas\\Authorization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AliChry\\Laminas\\Authorization\\Test\\": "test/",
            "LaminasTest\\Mvc\\": "vendor/laminas/laminas-mvc/test/"
        },
        "files": [
            "vendor/laminas/laminas-mvc/test/_autoload.php"
        ]
    }
}
